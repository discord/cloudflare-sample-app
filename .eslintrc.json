{
  "extends": ["prettier", "eslint:recommended", "plugin:security/recommended"],
  "plugins": ["prettier", "security"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "env": {
    "serviceworker": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "ignorePatterns": [
    "node_modules"
  ]
}
